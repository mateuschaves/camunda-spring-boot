{
  "type": "default",
  "components": [
    {
      "key": "money",
      "label": "Dinheiro",
      "type": "textfield",
      "validate": {
        "required": true,
        "pattern": "^(?:0|[1-9][0-9]*)\\.[0-9]+$"
      },
      "description": "Quanto você vai levar ?"
    },
    {
      "key": "country",
      "label": "Para onde você vai ?",
      "type": "textfield",
      "description": "Destino da viagem",
      "validate": {
        "required": true,
        "pattern": "Argentina|Mexico"
      }
    },
    {
      "key": "taxiCost",
      "label": "Preço do táxi",
      "type": "textfield",
      "validate": {
        "pattern": "^(?:0|[1-9][0-9]*)\\.[0-9]+$"
      },
      "description": ""
    }
  ]
}